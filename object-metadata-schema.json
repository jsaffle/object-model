{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://raw.githubusercontent.com/jsaffle/object-model/main/object-metadata-schema.json",
	"title": "Object type metadata schema",
	"description": "",
	"properties":
	{
		"metadataPrefix":
		{
			"$comment": ".",
			"const": "object-metadata"
		},
		"valuePrefix":
		{
			"$comment": ".",
			"type": "string",
			"pattern": "^$"
		},
		"className":
		{
			"$comment": ".",
			"type": "string",
			"pattern": "^((any)|(.+))$"
		},
		"formatSpecifier":
		{
			"$comment": "",
			"type": "string",
			"pattern": "^%.+$"
		}
	},
	"additionalProperties": false
}
